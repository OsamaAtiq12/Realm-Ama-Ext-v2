{
    "manifest_version": 3,
    "name": "Realm-ama-ext",
    "description": "Realm-ama-ext",
    "version": "1.0.0",
    "action" : {
       
        "default_title": "Realm-ama-ext",
        "default_icon" : "icon.png"
    },
  
    "permissions": [
        "tabs",
        "bookmarks",
        "storage",
        "identity",
        "management",
        "windows"
    ],
    "icons": {
        "16" : "icon.png",
        "48" : "icon.png",
        "128" : "icon.png"
    },
    "options_page": "options.html",
    "background": {
        "service_worker": "background.js"
    },
    "content_scripts": [
        {
            "matches": ["<all_urls>"],
            "js": ["contentScript.js"]
        }
    ],

    
    "commands": {
        "open_extension": {
          "suggested_key": {
            "default": "Ctrl+Shift+E",
            "mac": "MacCtrl+Shift+E"
          },
          "description": "Open the extension"
        }
      }

      
}